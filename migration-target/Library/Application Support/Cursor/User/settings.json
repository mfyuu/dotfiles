{
  "chat.editor.fontFamily": "'UDEV Gothic NFLG'",
  "css.lint.unknownAtRules": "ignore",
  "cursor.cpp.disabledLanguages": ["plaintext", "markdown", "scminput"],
  "debug.console.fontFamily": "'UDEV Gothic NFLG'",
  "debug.console.fontSize": 13,
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.accessibilitySupport": "off",
  "editor.codeLensFontFamily": "'UDEV Gothic NFLG'",
  "editor.cursorBlinking": "phase",
  "editor.cursorSmoothCaretAnimation": "explicit",
  "editor.experimentalInlineEdit.fontFamily": "'UDEV Gothic NFLG'",
  "editor.fontFamily": "'UDEV Gothic NFLG'",
  "editor.fontSize": 14,
  "editor.formatOnSave": true,
  "editor.inlineSuggest.fontFamily": "'UDEV Gothic NFLG'",
  "editor.renderLineHighlight": "all",
  "editor.renderLineHighlightOnlyWhenFocus": true,
  "explorer.confirmDragAndDrop": false,
  "extensions.ignoreRecommendations": true,
  "files.autoSave": "afterDelay",
  "fileutils.inputBox.pathType": "workspace",
  "fileutils.inputBox.pathTypeIndicator": "",
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "git.openRepositoryInParentFolders": "never",
  "github.copilot.chat.localeOverride": "ja",
  "github.copilot.editor.enableAutoCompletions": true,
  "github.copilot.enable": {
    "*": true,
    "markdown": false,
    "plaintext": false,
    "scminput": false
  },
  "githubPullRequests.createOnPublishBranch": "never",
  "githubPullRequests.pullBranch": "never",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "presentation-mode.active": {
    "commands": ["workbench.action.closeSidebar"],
    "editor.fontSize": 16,
    "editor.matchBrackets": "never",
    "editor.minimap.enabled": false,
    "editor.scrollbar.verticalScrollbarSize": 0,
    "window.zoomLevel": 2,
    "workbench.activityBar.visible": false,
    "workbench.colorCustomizations": {
      "[GitHub Dark Default]": {
        "activityBar.background": "#000",
        "editor.background": "#000",
        "editor.lineHighlightBackground": "#0000",
        "editor.lineHighlightBorder": "#0000",
        "editorWhitespace.foreground": "#59A5FC",
        "panel.background": "#000",
        "sideBar.background": "#000",
        "terminal.background": "#000"
      },
      "[GitHub Light Default]": {
        "activityBar.background": "#fff",
        "editor.background": "#fff",
        "editor.lineHighlightBackground": "#fff0",
        "editor.lineHighlightBorder": "#fff0",
        "editorWhitespace.foreground": "#1167D7",
        "panel.background": "#fff",
        "sideBar.background": "#fff",
        "terminal.background": "#fff"
      }
    },
    "workbench.colorTheme": "GitHub Dark Default",
    "workbench.statusBar.visible": false
  },
  "presentation-mode.inactive": {
    "commands": ["workbench.action.focusSideBar"]
  },
  "scm.inputFontFamily": "editor",
  "security.workspace.trust.untrustedFiles": "open",
  "tailwind-fold.autoFold": false,
  "tailwind-fold.unfoldedTextOpacity": 0.7,
  "terminal.external.osxExec": "Warp.app",
  "terminal.integrated.env.osx": {
    "Q_NEW_SESSION": "1"
  },
  "terminal.integrated.fontSize": 13,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "workbench.activityBar.orientation": "vertical",
  "workbench.colorTheme": "Default Dark Modern",
  "workbench.editor.enablePreview": false,
  "workbench.iconTheme": "material-icon-theme",
  "workbench.startupEditor": "none",
  "workbench.tree.indent": 24,
  "[dockercompose]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "editor.autoIndent": "advanced"
  },
  "editor.largeFileOptimizations": false,
  "explorer.fileNesting.patterns": {
    "*.ts": "${capture}.js",
    "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
    "*.jsx": "${capture}.js",
    "*.tsx": "${capture}.ts",
    "tsconfig.json": "tsconfig.*.json",
    "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, bun.lockb, bun.lock",
    "*.sqlite": "${capture}.${extname}-*",
    "*.db": "${capture}.${extname}-*",
    "*.sqlite3": "${capture}.${extname}-*",
    "*.db3": "${capture}.${extname}-*",
    "*.sdb": "${capture}.${extname}-*",
    "*.s3db": "${capture}.${extname}-*"
  },
  "claudeCode.selectedModel": "default",
  "claudeCode.preferredLocation": "panel",
  // リガチャ
  "editor.fontLigatures": true,
  "json.schemaDownload.enable": true,
  "explorer.confirmDelete": false
}
